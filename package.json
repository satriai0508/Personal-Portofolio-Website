{
  "name": "personal-portofolio",
  "version": "1.0.0",
  "description": "Personal Web Portofolio",
  "main": "index.html",
  "private": "true",
  "scripts": {
    "tailwind-dev": "npx tailwindcss -i ./src/main.css -o ./dist/style.css",
    "tailwind-watch": "npx tailwindcss -i ./src/main.css -o ./dist/style.css --watch",
    "tailwind-minify" : "npx tailwindcss -o dist/css/tailwind.css --minify"
