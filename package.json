{
  "name": "personal-portofolio",
  "version": "1.0.0",
  "description": "Personal Web Portofolio",
  "main": "index.html",
  "private": "true",
  "scripts": {
    "dev": "npx tailwindcss -i ./src/main.css -o ./dist/style.css",
    "watch": "npx tailwindcss -i ./src/main.css -o ./dist/style.css --watch",
    "prod": "npx tailwindcss -o dist/css/tailwind.css",
    "production": "npx tailwindcss -o dist/css/tailwind.css --minify"
  },
  "keywords": [
    "web",
    "tailwindcss",
    "portofolio",
    "personal web"
  ],
  "author": "Deuwi Satriya Irawan",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.4"
  }
}
